#include <iostream>
#include<string>
using namespace std;

int main(){
    int t;
    cin>>t;
    while(t--){
        string s,t,m=" ";
        cin>>s>>t;
        for(int i=0;i<s.length();i++){
            if(s[i]==t[i]){
                cout<<"g";
            }
            else{
                cout<<"b";
            }
        }
        cout<<m<<endl;
        
    }
    
	// your code goes here
	return 0;
}
